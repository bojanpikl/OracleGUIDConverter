{
	"name": "oracleguidconverter",
	"publisher": "piklbojan",
	"displayName": "Oracle Guid Converter",
	"description": "Convert Oracle GUID Raw(16) to .Net and vice versa",
	"version": "0.0.9",
	"icon": "assets/logo.png",
	"repository": "https://github.com/bojanpikl/OracleGUIDConverter",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"oracle",
		"convert",
		"guid",
		"guid converter",
		"convert raw(16)",
		"raw (16)",
		"raw 16"
	],
	"activationEvents": [
		"onCommand:oracleguidconverter.convert"
	],
	"browser": "./dist/web/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "oracleguidconverter.convert",
				"title": "Oracle Raw(16) GUID Converter"
			}
		]
	},
	"scripts": {
		"test": "jest",
		"pretest": "yarn run compile-web",
		"vscode:prepublish": "yarn run package-web",
		"compile-web": "webpack",
		"watch-web": "webpack --watch",
		"package-web": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts",
		"run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. .",
		"deploy": "vsce publish --yarn"
	},
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"@types/mocha": "^9.0.0",
		"@types/vscode": "^1.61.0",
		"@types/webpack-env": "^1.16.2",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"@vscode/test-web": "^0.0.13",
		"@vscode/vsce": "^2.24.0",
		"assert": "^2.0.0",
		"eslint": "^7.32.0",
		"jest": "^29.7.0",
		"mocha": "^9.1.1",
		"process": "^0.11.10",
		"ts-jest": "^29.1.2",
		"ts-loader": "^9.2.5",
		"typescript": "^4.9.5",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0"
	}
}
